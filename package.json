    "react-relay": "^11.0.2",
    "relay-nextjs": "^0.4.0",
    "relay-runtime": "^11.0.2",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@types/react": "17.0.14",
    "babel-plugin-relay": "^11.0.2",
    "eslint": "7.30.0",
    "eslint-config-next": "11.0.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "graphql": "^15.5.1",
